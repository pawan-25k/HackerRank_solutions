
int birthdayCakeCandles(vector<int> candles) {
    int tallest=candles[0];
    int count=0;
    for(int i=0;i<candles.size();i++){
        if (tallest<candles[i]){
          tallest=candles[i];
          count=1;
        }else if (tallest==candles[i]) {
            count+=1;
        
        }
    }
    return count;
    }
