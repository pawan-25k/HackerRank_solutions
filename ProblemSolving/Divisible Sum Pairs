#Completed 'divisibleSumPairs' function below
def divisibleSumPairs(n, k, ar):
    # Write your code here
    count=0
    for i in range(n):
        for j in range(i + 1, n):
            current_sum = ar[i] + ar[j]
            if current_sum % k == 0:
                count += 1
                
    return count
